{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "new-cap": "off",
    "block-spacing": "off",
    "prefer-const": "off",
    "no-var": "off",
    "max-len": ["error", {"code": 200}],
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "camelcase": "off",
    "brace-style": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-extend-native": "off",
    "no-invalid-this": "off",
    "object-curly-spacing": "off",
    "space-before-function-paren": "off",
    "one-var": "off",
    "operator-linebreak": "off",
    "no-use-before-define": "off",
    "no-throw-literal": "off",
    "no-loop-func": "off",
    "no-prototype-builtins": "off",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "no-multiple-empty-lines": "off",
    "guard-for-in": "off",
    "no-lonely-if": "off",
    "padded-blocks": "off",
    "no-floating-decimal": "off",
    "space-infix-ops": "off",
    "spaced-comment": "off",
    "no-multi-spaces": "off",
    "keyword-spacing": "off",
    "prefer-rest-params": "off",
    "prefer-spread": "off",
    "space-before-blocks": "off",
    "curly": "off",
    "no-else-return": "off",
    "no-nested-ternary": "off",
    "no-mixed-operators": "off",
    "no-useless-return": "off",
    "no-shadow": "off",
    "arrow-parens": "off",
    "no-unmodified-loop-condition": "off",
    "no-sequences": "off"
  }
}
